        android:id="@+id/nav_graficadora"
        android:name="com.example.graficadorestadistico.ui.Graficadora.PrincipalFragment"
        android:label="Graficadora"
        tools:layout="@layout/fragment_principal" />

    <!--era el gallery-->
    <fragment
        android:id="@+id/nav_manual"
        android:name="com.example.graficadorestadistico.ui.ManualUso.ManualUsoFragment"
